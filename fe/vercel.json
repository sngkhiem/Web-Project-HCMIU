{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "localhost:8080/api/:path*"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTION"],
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
} 